#!/usr/bin/env bash

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

alias sed='sed -r'

alias vim='vim -p'
alias tree='tree -I node_modules'

# A real clear-screen
alias cls='printf "\ec"'

modified()
{
  git ls-files -mo --exclude-standard
}

. ${HOME}/dotfiles/setprompt


